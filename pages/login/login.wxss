/* pages/login/login.wxss */
page {
  font-size: 20rpx;
  background-size: cover;
  background-image: url('data:image/jpeg;base64,')
}

button:not([size='mini']) {
  padding: 0;
  margin: 0;
}

.container {
  position: relative;
  padding: 10% 5%;
  margin: 50% auto;
  width: 90%;
  border: 20rpx solid hsla(0, 0%, 100%, 0.3);
  background-clip: padding-box;
  border-radius: 50rpx;
  background-color: #fff;
}

.container .title {
  position: absolute;
  width: 341rpx;
  height: 104rpx;
  top: -50rpx;
}

.input-wrapper {
  display: flex;
  margin-top: 30rpx;
  width: 100%;
  height: 80rpx;
  align-items: center;
  border: 2rpx solid #EAE7E8;
  border-radius: 80rpx;
}

.input-wrapper input {
  width: 60%
}

.input-wrapper image {
  width: 50rpx;
  height: 50rpx;
  margin: 0 20rpx;
}

.link-wrapper {
  display: flex;
  width: 90%;
  margin-top: 20rpx;
}

.link-wrapper .find-pass {
  width: 50%;
  text-align: left;
}

.link-wrapper .register {
  width: 100%;
  text-align: center;
  font-size: 28rpx;
  color: #4876FF;
}

.btn-login:not([size='mini']) {
  margin-top: 30rpx;
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  background-color: #4876FF;
  color: #fff;
  border-radius: 80rpx;
  font-size: 28rpx;
}

.login-container {
  width: 90%;
  margin: 300rpx auto;
  color: #fff;
  font-size: 32rpx;
}

.login-container .logo,
.login-container .btn-login,
.login-container .tip-msg {
  width: 100%;
  align-items: center;
  text-align: center;
  margin: 20rpx auto;
}

.login-container .logo image {
  width: 300rpx;
}

.login-container .btn-login {
  width: 70%;
}

.login-container .btn-login button {
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  background-color: #4876FF;
  color: #fff;
  border-radius: 45rpx;
  font-size: 28rpx;
}